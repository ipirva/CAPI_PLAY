## custom parameters for this cluster - you would not want to change these
# base name is chosen as a default name
# check to have the same as in clusterctl.yaml
# custom
export POD_CIDR=192.168.0.0/16
export AWS_CUSTOM_SG=base-custom
# default
export CLUSTER_NAME=base
export AWS_REGION=eu-west-2
export AWS_CONTROL_PLANE_MACHINE_TYPE=t3.medium
export AWS_NODE_MACHINE_TYPE=t3.medium
export AWS_SSH_KEY_NAME=base-ssh
export CONTROL_PLANE_MACHINE_COUNT=1
export WORKER_MACHINE_COUNT=1
export KUBERNETES_VERSION=v1.18.0
